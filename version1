makeCachematrix <- function(x = numeric()) {

  inverse <- NULL
  set <- function(y) {
  x <<- y
  inverse <<- NULL
  }
  
  get <- function() x
  setinverse <- function(minverse) inverse <<- minverse
  getinverse <- function() inverse
  
  list(set = set, get = get, setinverse = setinverse, getinverse = getinverse)
}
